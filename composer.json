{
    "name": "cesarbosqueiro/timeline-manager",
    "description": "This is my package timeline-manager",
    "keywords": [
        "cesarbosqueiro",
        "laravel",
        "timeline-manager"
    ],
    "homepage": "https://github.com/cesarbosqueiro/timeline-manager",
    "support": {
        "issues": "https://github.com/cesarbosqueiro/timeline-manager/issues",
        "source": "https://github.com/cesarbosqueiro/timeline-manager"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Cesar Bosqueiro",
            "email": "cesar@bosqro.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "illuminate/contracts": "^10.0|^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0|^8.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "CesarBosqueiro\\Timeline-Manager\\": "src/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CesarBosqueiro\\Timeline-Manager\\TimelineManagerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
